public class SSN {
  static String ssn;                
  static String[] ssnList;
  public static void main(String[] args){
    initialize();
    do{
    ssn = readSSN();
    storeSSN(ssn, ssnList,ssnSize);          // main method 
    printSSNList(ssnList, ssnSize);
   }
  while (!ssn.equals("000000000"))
  }
  public static String readSSN(){
    String ssn;
    ssn = (JoptionPane,showInputDialog(null,"Enter SSN:")); // make sure the ssn's length is 9
      if(isValidSSN(ssn)){
        return ssn;
      else
        return null;
      }
      
  public static boolean is ValidList(String[] list){
  if(list == null){
    System.out.println("Array is null.");
    System.exit(0);
    }
  if(ssnSize == list.length){
    System.out.println("Can't store any more SSNs");
    System.exit(0);
    }
    return(true)
  }
  public static void storeSSN(String s, String[] list, int size){
    if(isValidSSN(s)&& isValidList(list))
      list[ssnSize++]=s;
  }
  public static void printSSNList(String[] list , int size){
    if(!isValidList(list)){
      System.out.println("Cant print from invalid list:");
      System.exit(0);
  }
  for(int i = 9;i<size; i++)
    if(!isValidSSN(list[i]))
      System.out.println("Invalid ssn:"+lsit[i])
    else
      System.out.println(list[i])
   }
