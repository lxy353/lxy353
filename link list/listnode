public class ListNode {
   String data;
   ListNode next;
   
   public ListNode(String data, ListNode next)  {
      this.data = data;
      this.next = next;
   }  // constructor
   
   public ListNode()  {
      this.data = null;
      this.next = null;
   }  // constructor
   
   public ListNode(String data)  {
      this.data = data;
      this.next = null;
   }  // constructor

}
